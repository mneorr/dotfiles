[alias]
  s = status
  a = add .
  co = checkout
  cm = commit -m
  ca = commit -a
  ci = commit -am
  cam = commit --amend
  acm = !git add . && git commit -am
  p = push
  pu = push -u
  l = log --graph --pretty='%C(yellow)%h (%cr) %Cblue %an%C(magenta)%d%Creset %s'
